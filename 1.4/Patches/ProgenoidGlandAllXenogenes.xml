<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="VFECore.PatchOperationToggableSequence">
        <enabled>false</enabled>
        <mods>
            <li>Warhammer 40k - Genes</li>
        </mods>
        <label>Progenoid Glands copies all xenogenes from pawn:</label>
        <operations>
            <li Class="PatchOperationReplace">
                <xpath>Defs/RecipeDef[@Name = "BEWH_GeneseedFromProgenoid"]/workerClass</xpath>
                <value>
                    <workerClass>Genes40k.WorkerClass_GeneProgenoidRemoval_AllGenes</workerClass>
                </value>
            </li>
        </operations>
    </Operation>
</Patch>