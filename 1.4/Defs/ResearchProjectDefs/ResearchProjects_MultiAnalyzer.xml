<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Geneseed Creation Base -->
  <ResearchProjectDef Name="BEWH_GeneseedBase" Abstract="True">
    <baseCost>30000</baseCost>
    <techLevel>Spacer</techLevel>
    <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
    <requiredResearchFacilities>
      <li>MultiAnalyzer</li>
    </requiredResearchFacilities>
    <tab>BEWH_WH40kTab</tab>
  </ResearchProjectDef>


  <!-- Space Marine Creation -->
  <ResearchProjectDef ParentName="BEWH_GeneseedBase">
    <defName>BEWH_SpaceMarineCreation</defName>
    <label>Space Marine Geneseed Creation</label>
    <description>Unlocks recipes for creating your own Space Marine geneseeds at the fabrication bench.</description>
    <prerequisites>
      <li>AdvancedFabrication</li>
      <li>Archogenetics</li>
      <li>Bionics</li>
    </prerequisites>
    <researchViewX>3</researchViewX>
    <researchViewY>2</researchViewY>
  </ResearchProjectDef>


  <!-- Primaris Marine Creation -->
  <ResearchProjectDef ParentName="BEWH_GeneseedBase">
    <defName>BEWH_PrimarisMarineCreation</defName>
    <label>Primaris Marine Geneseed Creation</label>
    <description>Unlocks recipes for creating your own Primaris Marine geneseeds at the fabrication bench.</description>
    <prerequisites>
      <li>AdvancedFabrication</li>
      <li>Archogenetics</li>
      <li>Bionics</li>
      <li>BEWH_SpaceMarineCreation</li>
    </prerequisites>
    <researchViewX>1</researchViewX>
    <researchViewY>2</researchViewY>
  </ResearchProjectDef>

</Defs>